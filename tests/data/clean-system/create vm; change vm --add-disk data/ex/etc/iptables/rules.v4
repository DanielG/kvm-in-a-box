*nat
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
:PREROUTING ACCEPT [0:0]
:KIB_INPUT - [0:0]
:KIB_OUTPUT - [0:0]
:KIB_POSTROUTING - [0:0]
:KIB_PREROUTING - [0:0]
-A INPUT -j KIB_INPUT
-A OUTPUT -j KIB_OUTPUT
-A POSTROUTING -j KIB_POSTROUTING
-A PREROUTING -j KIB_PREROUTING
-A KIB_POSTROUTING -s 10.0.0.0/16 -o eth0 -j MASQUERADE
COMMIT
*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:KIB_INPUT - [0:0]
:KIB_FORWARD - [0:0]
:KIB_OUTPUT - [0:0]
-A INPUT -j KIB_INPUT
-A FORWARD -j KIB_FORWARD
-A OUTPUT -j KIB_OUTPUT
-A KIB_FORWARD -i eth0 -o kpu -j ACCEPT
-A KIB_FORWARD -i kpu -o eth0 -j ACCEPT
-A KIB_INPUT -i kpu -p udp -m udp --dport "67:68" -j ACCEPT
-A KIB_INPUT -i kpu -p udp -m udp --dport "53" -j ACCEPT
COMMIT
